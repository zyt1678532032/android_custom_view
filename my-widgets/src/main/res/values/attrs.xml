<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AvatarView">
        <attr name="avatar_type" format="enum">
            <enum name="normal" value="0" />
            <enum name="deposit" value="1" />
            <enum name="owner" value="2" />
        </attr>
        <attr name="src" format="reference|color" />
    </declare-styleable>
    <declare-styleable name="TimeLineView">
        <attr name="time_paddingStart" format="dimension" />
        <attr name="time_circleRadius" format="float" />
    </declare-styleable>

    <declare-styleable name="BatteryMeterView">
        <attr name="charging" format="boolean" />
        <attr name="warning_level" format="integer" />
        <attr name="battery_level" format="integer" />
        <attr name="normal_fill_color" format="color" />
        <attr name="warning_fill_color" format="color" />
        <attr name="background_fill_color" format="color" />
        <attr name="battery_head_color" format="color" />
        <attr name="charging_color" format="color" />
        <attr name="text_color" format="color" />
    </declare-styleable>

    <declare-styleable name="SlidingMenu">
        <attr name="fillViewport" format="boolean" />
    </declare-styleable>
</resources>